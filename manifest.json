{
  "name": "Trollbot",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "profile.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_title": "Trollbot"
  }
}